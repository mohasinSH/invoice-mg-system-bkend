{
    "version": 2,
    "builds": [
      {
        "src": "pg-database.js",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "DB_HOST": "@db_host",
      "DB_USER": "@db_user",
      "DB_PASSWORD": "@db_password",
      "DB_NAME": "@db_name",
      "DB_PORT": "@db_port"
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/"
      }
    ]
  }
  